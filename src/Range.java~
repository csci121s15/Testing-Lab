public class Range {
  public Range(double start, double stop) {
    {
    if (start > stop)
    {
     max = start;
      min = stop;
   }
    
    if (stop > start)
   {
     max = stop;
     min = start;
   }
   
   if (stop == start)
   {
   min = 0;
   max = 0;
   }
 }
 
  
  public boolean contains (double value) {
    return true;
  }
  
  public double getWidth () {
   return 0.0; 
  }
  
  public double getMin() {
    return 0.0;
  }
  public double getMax() {
    return 0.0;
  }
  public Range intersection(Range other) {
    return new Range(0.0, 0.0);
  }
}
      
  
    